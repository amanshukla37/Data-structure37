public:
    vector<int> singleNumber(vector<int>& nums) {
        vector<int>arr;
       sort(nums.begin(),nums.end());
       for(int i=0;i<nums.size()-1;i++)
       {
          if(nums[i]==nums[i+1])
          {
              i++;
          }
          else{
          arr.push_back(nums[i]);
          if(arr.size()==2)
          return arr;}
       } 
       if(nums[nums.size()-1]!=nums[nums.size()-2])
       arr.push_back(nums[nums.size()-1]);
       return arr;
    }
